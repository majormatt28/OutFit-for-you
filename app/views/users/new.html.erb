<h1>New User</h1>

<%= form_for @user do |f|%>
<%= f.label "Name: "%>
<%= f.text_field :name%>
<br>
<%= f.label "Gender: "%>
<%# f.collection_select :user_id, @users, :id, :gender %>
<br>
<%= f.label "Profile Pic: "%>
<%= f.text_field :img_url%>
<br>
<%= f.label "Email Address: "%>
<%= f.text_field :email%>
<br>
<%= f.label "Password: "%>
<%= f.text_field :password_digest%>
<br>
<%= f.submit%>
<%end%>
